<Shapes xmlns="http://www.evolus.vn/Namespace/Pencil" xmlns:p="http://www.evolus.vn/Namespace/Pencil" xmlns:svg="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" id="andresdevr_html5_pencil_stencils" displayName="HTML5" description="Provides HTML5 UI components" author="Andres LÃ³pez" url="https://github.com/andresdevr/HTML5-Pencil-Stencils"><Properties></Properties><Script></Script><Shape id="Navbar" displayName="Navbar" icon="Icons/Navbar.png">
	<Properties>
		<PropertyGroup>
			<Property name="box" type="Dimension">1024,50</Property>
			<Property name="smartColor" displayName="Automatic border color" type="Bool">true</Property>
			<Property name="fullWidth" displayName="Full-width" type="Bool">false</Property>
			<Property name="radius" displayName="Corner Radius" type="Handle" p:lockY="true" p:minX="0" p:maxX="$box.h / 2" p:noScale="true">4,0</Property>
		</PropertyGroup>
		<PropertyGroup name="Background">
			<Property name="fillColor" displayName="Color" type="Color">#f8f8f8ff</Property>
		</PropertyGroup>
		<PropertyGroup name="Border">
			<Property name="strokeColor" displayName="Border Color" type="Color"><E>$$strokeColor</E></Property>
			<Property name="strokeStyle" displayName="Border Style" type="StrokeStyle">1|</Property>
		</PropertyGroup>
	</Properties>
	<Behaviors>
		<For ref="bg">
			<Box>$box</Box>
			<Radius>
				<Arg>$radius.x</Arg>
				<Arg>$radius.x</Arg>
			</Radius>
			<Fill>$fillColor</Fill>
			<StrokeColor>$smartColor.value ? collection.darken($fillColor, 8) : $strokeColor</StrokeColor>
			<StrokeStyle>$fullWidth.value ? StrokeStyle.fromString("0|") : $strokeStyle</StrokeStyle>
		</For>
		<For ref="borderBottom">
			<Visibility>$fullWidth.value</Visibility>
			<D>[M($radius.x, $box.h), L($box.w - $radius.x, $box.h)]</D>
			<StrokeColor>$smartColor.value ? collection.darken($fillColor, 8) : $strokeColor</StrokeColor>
			<StrokeStyle>$strokeStyle</StrokeStyle>
		</For>
	</Behaviors>

	<p:Content	xmlns:p="http://www.evolus.vn/Namespace/Pencil"
									xmlns="http://www.w3.org/2000/svg">
		<rect id="bg" />
		<path id="borderBottom" />
	</p:Content>
</Shape></Shapes>